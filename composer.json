{
    "name": "saranchyev/planfix-rest-api-generated",
    "description": "automatically generated (Open API) REST API client for https://planfix.ru/",
    "keywords": [
        "planfix",
        "rest",
        "api",
        "php"
    ],
    "homepage": "https://github.com/saranchyev/planfix-rest-api-generated",
    "license": "unlicense",
    "authors": [
        {
            "name": "Ilya Saranchyev",
            "homepage": "https://github.com/saranchyev"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
